{
    "family": "dev-media-service",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "dev-media-service",
            "cpu": 256,
            "memoryReservation": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort":80,
                    "hostPort": 0
                }
            ]
        }
    ]
}

